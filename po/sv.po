msgid "Always"
msgstr "Alltid"

#: ../src/configuration-gui/abrt-config-widget.glade.h:15
msgid "Never"
msgstr "Aldrig"

#: ../src/configuration-gui/abrt-config-widget.glade.h:16
msgid "Ask"
msgstr "Fråga"

#: ../src/configuration-gui/abrt-config-widget.glade.h:17
