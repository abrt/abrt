msgstr "提交报告"

#: ../src/plugins/post_report.xml.in.h:2
msgid "Executed after the reporting is finished"
msgstr "完成报告后执行"

#: ../src/plugins/post_report.xml.in.h:3
msgid "Used for updating of the databases"
msgstr "用于更新数据库"
